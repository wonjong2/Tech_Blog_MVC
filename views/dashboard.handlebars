<div class="container-fluid">
    {{#each myPosts}}
        <div class="card mt-3 mx-5" data-post-id="{{this.id}}">
            <div
                class="d-flex justify-content-between align-items-center card-header bg-secondary"
            >
                <p class="title mb-0">{{this.title}}</p>
                <p class="user-date mb-0">{{format_date this.date}}</p>
            </div>
        </div>
    {{/each}}

    <button type="button" class="btn btn-primary btn-lg" id="new-post">Large
        button</button>
</div>

<script src="./js/dashboard.js" type="text/javascript"></script>